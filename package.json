{
  "name": "forex-project",
  "version": "1.0.0",
  "scripts": {
    "prestart": "npm install",
    "start": "npm run watch:all",
    "build:ts": "tsc",
    "build:sass": "sass src/styles/styles.scss dist/styles/styles.css",
    "watch:ts": "tsc --watch",
    "watch:sass": "sass --watch src/styles/styles.scss:dist/styles/styles.css",
    "watch:all": "concurrently \"npm run watch:ts\" \"npm run watch:sass\""
  },
  "devDependencies": {
    "concurrently": "^7.0.0",
    "sass": "^1.32.0",
    "typescript": "^4.0.0"
  }
}
